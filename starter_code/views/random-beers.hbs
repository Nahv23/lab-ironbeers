<div class="index-container">

    <nav class="navbar navbar-expand-lg navbar-light">
      <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <div class="navbar-nav">
        <a class="text-links" href="/">Home</a>
        <a class="text-links" href="/Beers">Beers</a>
        <a class="text-links active" href="/random-beers">Random Beer <span class="sr-only">(current)</span></a>
        </div>
      </div>
    </nav>
</div>

<div>
  <div class="img-random">
     <img src="{{this.image_url}}" alt="{{this.name}}">
  </div>
 <div class="card-style" style="width: 40rem;">
   <div class="card-body">
    <h3 class="card-title">{{this.name}}</h3>
    <h6 class="card-subtitle mb-2">{{this.tagline}}</h6>
    <p class="card-text">{{this.description}}</p>
    <p><strong>Food Pairing</strong></p>
      <ul>
        {{#each this.food_pairing}}
          <li>{{this}}</li>
        {{/each}}
      </ul>
    <p><strong>Brewers Tips:</strong>{{this.brewers_tips}}</p>
    
  </div>
 </div>
</div>
